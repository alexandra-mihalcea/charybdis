{
  "manifest_version": 2,
  "name": "Charybdis",
  "description": "",
  "version": "1",
  "author": "Alexandra Mihalcea",
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com https://use.fontawesome.com/ https://cdnjs.cloudflare.com/; object-src 'self'",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Charybdis"
  },
  "chrome_url_overrides" : {
    "newtab": "tab.html"
  },
  "permissions": ["activeTab", "tabs", "https://wall.alphacoders.com/", "storage"]
}